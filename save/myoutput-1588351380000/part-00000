or]]\n| location = [[Brewham]]\n| type = Cross\n| completed = {{sort|30|[[Middle Ages]]}}\n| show_architect = no\n| architect =\n| show_listed = no\n| date_listed = 1 November 1966\n| grid_ref = ST 71977 36126\n| lat = 51.1237\n| lon = -2.4018\n| show_notes = yes\n| notes = The cross has a rectangular base supporting an octagonal shaft from which the head is missing.\n| uid = 1018924\n| show_img = yes <!-- if using image column -->\n| image = Cross, The Church of St John the Baptist (geograph 2438762).jpg\n| upload_desc = <!-- Default to {{Unlink|{{{name|}}}}}. -->\n| show_ref = yes <!-- if using reference column -->\n| reference =<ref>{{NHLE|desc=Cross in the churchyard of St John the Baptist 15&nbsp;m south east of the church door|num=1018924|accessdate=16 November 2014}}</ref>\n| commonscat = <!-- category on Commons, *without* Category: prefix -->\n| show_wikidata= no\n| wikidata =\n}}\n{{EH listed building row\n| name = [[Lytes Cary|Deserted medieval settlement and associated fields, Lytes Cary]]\n| location = [[Lytes Cary]]\n| type = [[Earthworks (archaeology)|Earthworks]]\n| completed = {{sort|30|[[Middle Ages]]}}\n| show_architect = no\n| architect =\n| show_listed = no\n| date_listed = 22 March 1994\n| grid_ref = ST 53126 26545\n| lat = 51.0363\n| lon = -2.6699\n| show_notes = yes\n| notes = [[Earthworks (archaeology)|Earthworks]] within the grounds of [[Lytes Cary]] date from the medieval period. They now consist of low grassy banks.\n| uid = 1008253\n| show_img = yes <!-- if using image column -->\n| image =\n| upload_desc = <!-- Default to {{Unlink|{{{name|}}}}}. -->\n| show_ref = yes <!-- if using reference column -->\n| reference =<ref>{{NHLE | num=1008253|desc=Deserted medieval settlement and associated fields, Lytes Cary|accessdate=4 May 2013}}</ref><ref>{{cite web|title=Deserted medieval settlement and associated fields, Lytes Cary, Charlton Mackrell|url=http://www.somersetheritage.org.uk/record/53683|work=Somerset Historic Environment Record|publis